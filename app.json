{
    "expo": {
        "name": "tlccrm",
        "slug": "tlccrm",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
        "userInterfaceStyle": "light",
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.tlccrm.org"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.tlccrm.org"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
